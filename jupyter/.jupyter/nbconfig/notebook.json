{
  "CodeCell": {
    "cm_config": {
      "lineNumbers": true,
      "autoCloseBrackets": false
    }
  },
  "Notebook": {
    "Header": false,
    "Toolbar": true
  },
  "Cell": {
    "cm_config": {
      "lineNumbers": true
    }
  },
  "keys": {
    "command": {
      "unbind": [
        "q"
      ],
      "bind": {
        "q": "jupyter-notebook:clear-cell-output",
        "shift-r": "jupyter-notebook:rename-notebook",
        "shift-q": "jupyter-notebook:clear-all-cells-output"
      }
    }
  }
}